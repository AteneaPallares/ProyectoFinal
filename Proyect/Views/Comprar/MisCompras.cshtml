@model Dato
 @{

    ViewData["Title"] = "Mis Compras";

}
<div>
         <table class="table table-hover">
      <thead>
          <tr>
              <td><b>Nombre</b></td>
              <td><b>Marca</b></td>
              <td><b>Precio</b></td>
              <td><b>Id del Producto:</b></td>
              <td><b>Tama√±o:</b></td>
              <td><b>Comprada:</b></td>
              <td><b>Imagen</b></td>
          </tr>
      </thead>
    @foreach(var items in ViewBag.lista){
        @if(ViewBag.identidad==items.OwnerId){
        <tr>
            
            <td>@items.Nombre</td>
            <td>@items.Marca</td>
            <td>@items.precio</td>
             <td>@items.id_del_producto</td>
             <td>@items.Tamano</td>
             <td>@items.Comprada</td>
             <td><img src="@items.imagen" /> </td>
             
        </tr>
        }
    }
    </table>
</div>